#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/marvinbuth/Downloads/BTC-ECHO-Testaufgabe/02_studio/node_modules/.pnpm/sanity@4.6.0_@emotion+is-prop-valid@1.2.2_@portabletext+sanity-bridge@1.1.7_@sanity+schema@4._g5p3kjb6mym6iriwhcm2bceuty/node_modules/sanity/bin/node_modules:/Users/marvinbuth/Downloads/BTC-ECHO-Testaufgabe/02_studio/node_modules/.pnpm/sanity@4.6.0_@emotion+is-prop-valid@1.2.2_@portabletext+sanity-bridge@1.1.7_@sanity+schema@4._g5p3kjb6mym6iriwhcm2bceuty/node_modules/sanity/node_modules:/Users/marvinbuth/Downloads/BTC-ECHO-Testaufgabe/02_studio/node_modules/.pnpm/sanity@4.6.0_@emotion+is-prop-valid@1.2.2_@portabletext+sanity-bridge@1.1.7_@sanity+schema@4._g5p3kjb6mym6iriwhcm2bceuty/node_modules:/Users/marvinbuth/Downloads/BTC-ECHO-Testaufgabe/02_studio/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/marvinbuth/Downloads/BTC-ECHO-Testaufgabe/02_studio/node_modules/.pnpm/sanity@4.6.0_@emotion+is-prop-valid@1.2.2_@portabletext+sanity-bridge@1.1.7_@sanity+schema@4._g5p3kjb6mym6iriwhcm2bceuty/node_modules/sanity/bin/node_modules:/Users/marvinbuth/Downloads/BTC-ECHO-Testaufgabe/02_studio/node_modules/.pnpm/sanity@4.6.0_@emotion+is-prop-valid@1.2.2_@portabletext+sanity-bridge@1.1.7_@sanity+schema@4._g5p3kjb6mym6iriwhcm2bceuty/node_modules/sanity/node_modules:/Users/marvinbuth/Downloads/BTC-ECHO-Testaufgabe/02_studio/node_modules/.pnpm/sanity@4.6.0_@emotion+is-prop-valid@1.2.2_@portabletext+sanity-bridge@1.1.7_@sanity+schema@4._g5p3kjb6mym6iriwhcm2bceuty/node_modules:/Users/marvinbuth/Downloads/BTC-ECHO-Testaufgabe/02_studio/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../sanity/bin/sanity" "$@"
else
  exec node  "$basedir/../sanity/bin/sanity" "$@"
fi
