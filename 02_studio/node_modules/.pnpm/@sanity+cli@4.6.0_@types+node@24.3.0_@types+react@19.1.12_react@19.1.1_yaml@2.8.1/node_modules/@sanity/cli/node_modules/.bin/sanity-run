#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/marvinbuth/Downloads/BTC-ECHO-Testaufgabe/02_studio/node_modules/.pnpm/@sanity+runtime-cli@10.4.1_@types+node@24.3.0_debug@4.4.1_yaml@2.8.1/node_modules/@sanity/runtime-cli/bin/node_modules:/Users/marvinbuth/Downloads/BTC-ECHO-Testaufgabe/02_studio/node_modules/.pnpm/@sanity+runtime-cli@10.4.1_@types+node@24.3.0_debug@4.4.1_yaml@2.8.1/node_modules/@sanity/runtime-cli/node_modules:/Users/marvinbuth/Downloads/BTC-ECHO-Testaufgabe/02_studio/node_modules/.pnpm/@sanity+runtime-cli@10.4.1_@types+node@24.3.0_debug@4.4.1_yaml@2.8.1/node_modules/@sanity/node_modules:/Users/marvinbuth/Downloads/BTC-ECHO-Testaufgabe/02_studio/node_modules/.pnpm/@sanity+runtime-cli@10.4.1_@types+node@24.3.0_debug@4.4.1_yaml@2.8.1/node_modules:/Users/marvinbuth/Downloads/BTC-ECHO-Testaufgabe/02_studio/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/marvinbuth/Downloads/BTC-ECHO-Testaufgabe/02_studio/node_modules/.pnpm/@sanity+runtime-cli@10.4.1_@types+node@24.3.0_debug@4.4.1_yaml@2.8.1/node_modules/@sanity/runtime-cli/bin/node_modules:/Users/marvinbuth/Downloads/BTC-ECHO-Testaufgabe/02_studio/node_modules/.pnpm/@sanity+runtime-cli@10.4.1_@types+node@24.3.0_debug@4.4.1_yaml@2.8.1/node_modules/@sanity/runtime-cli/node_modules:/Users/marvinbuth/Downloads/BTC-ECHO-Testaufgabe/02_studio/node_modules/.pnpm/@sanity+runtime-cli@10.4.1_@types+node@24.3.0_debug@4.4.1_yaml@2.8.1/node_modules/@sanity/node_modules:/Users/marvinbuth/Downloads/BTC-ECHO-Testaufgabe/02_studio/node_modules/.pnpm/@sanity+runtime-cli@10.4.1_@types+node@24.3.0_debug@4.4.1_yaml@2.8.1/node_modules:/Users/marvinbuth/Downloads/BTC-ECHO-Testaufgabe/02_studio/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@sanity+runtime-cli@10.4.1_@types+node@24.3.0_debug@4.4.1_yaml@2.8.1/node_modules/@sanity/runtime-cli/bin/run.js" "$@"
else
  exec node  "$basedir/../../../../../../@sanity+runtime-cli@10.4.1_@types+node@24.3.0_debug@4.4.1_yaml@2.8.1/node_modules/@sanity/runtime-cli/bin/run.js" "$@"
fi
