#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/marvinbuth/Downloads/BTC-ECHO-Testaufgabe/02_studio/node_modules/.pnpm/get-folder-size@5.0.0/node_modules/get-folder-size/bin/node_modules:/Users/marvinbuth/Downloads/BTC-ECHO-Testaufgabe/02_studio/node_modules/.pnpm/get-folder-size@5.0.0/node_modules/get-folder-size/node_modules:/Users/marvinbuth/Downloads/BTC-ECHO-Testaufgabe/02_studio/node_modules/.pnpm/get-folder-size@5.0.0/node_modules:/Users/marvinbuth/Downloads/BTC-ECHO-Testaufgabe/02_studio/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/marvinbuth/Downloads/BTC-ECHO-Testaufgabe/02_studio/node_modules/.pnpm/get-folder-size@5.0.0/node_modules/get-folder-size/bin/node_modules:/Users/marvinbuth/Downloads/BTC-ECHO-Testaufgabe/02_studio/node_modules/.pnpm/get-folder-size@5.0.0/node_modules/get-folder-size/node_modules:/Users/marvinbuth/Downloads/BTC-ECHO-Testaufgabe/02_studio/node_modules/.pnpm/get-folder-size@5.0.0/node_modules:/Users/marvinbuth/Downloads/BTC-ECHO-Testaufgabe/02_studio/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/get-folder-size.js" "$@"
else
  exec node  "$basedir/../../bin/get-folder-size.js" "$@"
fi
