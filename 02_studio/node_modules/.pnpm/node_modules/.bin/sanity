#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/marvinbuth/Downloads/BTC-ECHO-Testaufgabe/02_studio/node_modules/.pnpm/@sanity+cli@4.6.0_@types+node@24.3.0_@types+react@19.1.12_react@19.1.1_yaml@2.8.1/node_modules/@sanity/cli/bin/node_modules:/Users/marvinbuth/Downloads/BTC-ECHO-Testaufgabe/02_studio/node_modules/.pnpm/@sanity+cli@4.6.0_@types+node@24.3.0_@types+react@19.1.12_react@19.1.1_yaml@2.8.1/node_modules/@sanity/cli/node_modules:/Users/marvinbuth/Downloads/BTC-ECHO-Testaufgabe/02_studio/node_modules/.pnpm/@sanity+cli@4.6.0_@types+node@24.3.0_@types+react@19.1.12_react@19.1.1_yaml@2.8.1/node_modules/@sanity/node_modules:/Users/marvinbuth/Downloads/BTC-ECHO-Testaufgabe/02_studio/node_modules/.pnpm/@sanity+cli@4.6.0_@types+node@24.3.0_@types+react@19.1.12_react@19.1.1_yaml@2.8.1/node_modules:/Users/marvinbuth/Downloads/BTC-ECHO-Testaufgabe/02_studio/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/marvinbuth/Downloads/BTC-ECHO-Testaufgabe/02_studio/node_modules/.pnpm/@sanity+cli@4.6.0_@types+node@24.3.0_@types+react@19.1.12_react@19.1.1_yaml@2.8.1/node_modules/@sanity/cli/bin/node_modules:/Users/marvinbuth/Downloads/BTC-ECHO-Testaufgabe/02_studio/node_modules/.pnpm/@sanity+cli@4.6.0_@types+node@24.3.0_@types+react@19.1.12_react@19.1.1_yaml@2.8.1/node_modules/@sanity/cli/node_modules:/Users/marvinbuth/Downloads/BTC-ECHO-Testaufgabe/02_studio/node_modules/.pnpm/@sanity+cli@4.6.0_@types+node@24.3.0_@types+react@19.1.12_react@19.1.1_yaml@2.8.1/node_modules/@sanity/node_modules:/Users/marvinbuth/Downloads/BTC-ECHO-Testaufgabe/02_studio/node_modules/.pnpm/@sanity+cli@4.6.0_@types+node@24.3.0_@types+react@19.1.12_react@19.1.1_yaml@2.8.1/node_modules:/Users/marvinbuth/Downloads/BTC-ECHO-Testaufgabe/02_studio/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@sanity/cli/bin/sanity" "$@"
else
  exec node  "$basedir/../@sanity/cli/bin/sanity" "$@"
fi
